<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_main">

    <fragment
        android:id="@+id/navigation_main"
        android:name="com.starrynight.tourapiproject.MainFragment"
        android:label="@string/title_main"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.starrynight.tourapiproject.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_star"
        android:name="com.starrynight.tourapiproject.starPage.TonightSkyFragment"
        android:label="@string/title_star"
        tools:layout="@layout/fragment_tonight_sky" />

    <fragment
        android:id="@+id/navigation_person"
        android:name="com.starrynight.tourapiproject.PersonFragment"
        android:label="@string/title_person"
        tools:layout="@layout/fragment_person" />
</navigation>